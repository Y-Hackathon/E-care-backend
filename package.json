{
  "name": "Yassir_E_Care_Backend",
  "version": "0.0.1",
  "description": "template for the backend of team E-care in the yassir internal hackaton",
  "main": "index.ts",
  "scripts": {
    "start": "node ./dist/index.js",
    "nodemon": "nodemon",
    "dev": "npm-run-all --parallel nodemon build:dev",
    "lint": "eslint --fix src/",
    "build": "webpack --env.ENVIRONMENT=production --config webpack.config.js --progress",
    "build:dev": "webpack --watch --env.ENVIRONMENT=development --config webpack.config.js",
    "test": "mocha -r ts-node/register tests/**/*.test.ts",
    "watch:test": "mocha -r ts-node/register --watch --watch-files src,tests tests/**/*.test.ts"
  },
  "keywords": [
    "E-care"
  ],
  "author": "Faith Omojola",
  "license": "ISC",
  "devDependencies": {
    "@types/express": "4.17.7",
    "@types/mocha": "7.0.2",
    "@types/mock-require": "2.0.0",
    "@types/node": "14.0.19",
    "@types/sinon": "9.0.4",
    "@types/supertest": "2.0.10",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "3.6.0",
    "@typescript-eslint/parser": "3.6.0",
    "clean-webpack-plugin": "3.0.0",
    "dotenv": "8.2.0",
    "eslint": "7.4.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-prettier": "3.1.4",
    "mocha": "8.0.1",
    "mock-require": "3.0.3",
    "nodemon": "2.0.4",
    "npm-run-all": "4.1.5",
    "prettier": "2.0.5",
    "sinon": "9.0.2",
    "supertest": "4.0.2",
    "ts-loader": "7.0.5",
    "ts-node": "8.10.2",
    "typescript": "3.9.6",
    "webpack": "4.43.0",
    "webpack-cli": "3.3.12",
    "webpack-node-externals": "1.7.2"
  },
  "dependencies": {
    "@google-cloud/pubsub": "2.1.0",
    "body-parser": "1.19.0",
    "dayjs": "^1.11.6",
    "express": "4.17.1",
    "firebase": "^9.14.0",
    "firebase-admin": "^11.2.1",
    "http-status-codes": "^2.2.0",
    "joi": "^17.7.0",
    "nanoid": "^4.0.0",
    "uuid": "^9.0.0"
  }
}
